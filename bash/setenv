#! /usr/bin/env bash


if which pass &> /dev/null; then

  #functions
  function setenv {
    source <(pass env/$1)
    echo "Env set to $1"
  }
  complete -W "$(pass ls env/ | awk '{print $2}')" setenv

  function securerun {
    pass bin/$1 | bash
  }
  complete -W "$(pass ls bin/ | awk '{print $2}')" securerun

fi

